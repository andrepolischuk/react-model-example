{
  "name": "react-model-example",
  "version": "0.0.0",
  "description": "An example of React async application",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "start": "cross-env NODE_ENV=development webpack-dev-server -d --hot",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrepolischuk/react-model-example.git"
  },
  "author": "Andrey Polischuk <me@andrepolischuk.com> (https://andrepolischuk.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrepolischuk/react-model-example/issues"
  },
  "homepage": "https://github.com/andrepolischuk/react-model-example",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "postcss-cssnext": "^2.5.2",
    "postcss-loader": "^0.9.1",
    "standard": "^10.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "async-to-promises",
      "transform-object-rest-spread",
      "transform-class-properties"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
